      {
        "name": "development",
        "slug": "dev",
        "stack_slug": "main"
      },
      {
        "name": "staging",
        "slug": "stage",
        "stack_slug": "main"
      },
      {
        "name": "production",
        "slug": "prod",
        "stack_slug": "main"
      }
    ]
  },
  "tfvars": {},
  "_extensions": ["cookiecutter.extensions.SlugifyExtension"]
